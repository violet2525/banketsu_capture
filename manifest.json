{
  "manifest_version": 2,
  "name": "バンケツくんキャプチャ",
  "version": "0.1.1",
  "description": "バンケツの画面スクショを撮る",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "downloads",
    "<all_urls>"
  ],
  "background": {
      "scripts": [ 
        "jquery.min.js",
        "background.js" 
      ],
      "persistent": false
  },
  "icons" : {
    "128": "icon.png"
  }
}